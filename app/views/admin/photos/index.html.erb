<h1>Listing photos</h1>

<table>
  <tr>
    <th>Taker</th>
    <th>Taker</th>
    <th>Taker no</th>
    <th>Location</th>
    <th>Name</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.taker %></td>
    <td><%= photo.taker_id %></td>
    <td><%= photo.taker_no %></td>
    <td><%= photo.location %></td>
    <td><%= photo.name %></td>
    <td><%= photo.user_id %></td>
    <td><%= link_to 'Show', [:admin,photo] %></td>
    <td><%= link_to 'Edit', edit_admin_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', [:admin,photo], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

